{
  "kind": "collectionType",
  "collectionName": "shippings",
  "info": {
    "singularName": "shipping",
    "pluralName": "shippings",
    "displayName": "Shipping",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "Price": {
      "type": "integer",
      "min": 0
    },
    "removedAt": {
      "type": "date"
    },
    "IsActive": {
      "type": "boolean"
    },
    "orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order.order",
      "mappedBy": "shipping"
    },
    "discounts": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::discount.discount",
      "mappedBy": "delivery_methods"
    }
  }
}
