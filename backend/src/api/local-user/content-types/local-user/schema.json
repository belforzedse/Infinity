{
  "kind": "collectionType",
  "collectionName": "local_users",
  "info": {
    "singularName": "local-user",
    "pluralName": "local-users",
    "displayName": "LocalUser",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "Phone": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "Password": {
      "type": "string",
      "private": true
    },
    "IsVerified": {
      "type": "boolean",
      "default": false
    },
    "product_reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product-review.product-review",
      "mappedBy": "user"
    },
    "user_wallet": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::local-user-wallet.local-user-wallet",
      "mappedBy": "user"
    },
    "contracts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::contract.contract",
      "mappedBy": "local_user"
    },
    "user_addresses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::local-user-address.local-user-address",
      "mappedBy": "user"
    },
    "IsActive": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "user_role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::local-user-role.local-user-role",
      "inversedBy": "users"
    },
    "cart": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::cart.cart",
      "mappedBy": "user"
    },
    "orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order.order",
      "mappedBy": "user"
    },
    "external_id": {
      "type": "string",
      "unique": false
    },
    "external_source": {
      "type": "string"
    },
    "removedAt": {
      "type": "datetime"
    },
    "strapi_user": {
      "type": "integer",
      "unique": true
    }
  }
}
